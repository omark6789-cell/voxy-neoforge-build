{
  "schemaVersion": 1,
  "id": "voxy",
  "version": "${version}",
  "custom": {
    "commit": "${commit}",
    "buildtime": "${buildtime}"
  },
  "name": "Voxy",
  "description": "Far distance rendering mod utilising LoDs",
  "authors": [
    "Cortex"
  ],
  "contact": {
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/voxy/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "me.cortex.voxy.client.VoxyClient"
    ],
    "main": [
      "me.cortex.voxy.commonImpl.VoxyCommon"
    ]
  },
  "mixins": [
    {
      "config": "client.voxy.mixins.json",
      "environment": "client"
    },
    "common.voxy.mixins.json"
  ],
  "depends": {
    "minecraft": ["1.21.1"],
    "fabricloader": ">=0.14.22",
    "fabric-api": ">=0.91.1",
    "sodium": ">=0.6.13"
  },
  "accessWidener": "voxy.accesswidener"
}
